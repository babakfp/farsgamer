{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$partials": ["src/partials"],
			"$partials/*": ["src/partials/*"],
			"$layouts": ["src/layouts"],
			"$layouts/*": ["src/layouts/*"],
			"$store": ["src/store"],
			"$store/*": ["src/store/*"],
			"$utils": ["src/utils"],
			"$utils/*": ["src/utils/*"],
			"$db": ["src/store/db"],
			"$db/*": ["src/store/db/*"]
		}
	},
	"extends": "./.svelte-kit/tsconfig.json",
	"include": ["src/**/*.js", "src/**/*.svelte"]
}
