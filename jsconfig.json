{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$layouts": ["src/layouts"],
			"$layouts/*": ["src/layouts/*"],
			"$store": ["src/store"],
			"$store/*": ["src/store/*"],
			"$utils": ["src/utils"],
			"$utils/*": ["src/utils/*"],
			"accordion": ["src/lib/accordion/index.js"],
			"accordion/*": ["src/lib/accordion/*"]
		}
	},
	"extends": "./.svelte-kit/tsconfig.json",
	"include": ["src/**/*.js", "src/**/*.svelte"]
}
